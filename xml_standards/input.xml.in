    <rsgis:command algor="rastergis_old" option="popattributestatsthresholded" input="image.env" clumps="clumps.env" table="input.att" [tableout="output.att"] >
        <rsgis:band band="int" threshold="float" count="field" [min="field"] [max="field"] [mean="field"] [sum="field"] [stddev="field"] />
