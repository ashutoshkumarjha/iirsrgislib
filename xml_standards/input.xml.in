    <rsgis:command algor="rastergis_old" option="popattributemean" input="image.env" clumps="clumps.env" output="output.att" prefix="string" />
    <!-- A command to export table attributes to an image -->
    <rsgis:command algor="rastergis_old" option="exporttable2img" clumps="clumps.env" table="table.att" output="image.env" format="GDAL Format" >
        <rsgis:band attribute="name" />
        <rsgis:band attribute="name" />
        <rsgis:band attribute="name" />
    </rsgis:command>
    <!-- A command to classify the attribute table using boolean logic  -->
    <rsgis:command algor="rastergis_old" option="boolclasstable" table="input.att" [tableout="output.att"] >
        <rsgis:if field="string" value="number">
            <rsgis:exp type="and"></rsgis:exp>
            <rsgis:exp type="or"></rsgis:exp>
            <rsgis:exp type="not"></rsgis:exp>
            <rsgis:exp type="gt" field1="string" field2="string" />
            <rsgis:exp type="lt" field1="string" field2="string" />
            <rsgis:exp type="gteq" field1="string" field2="string" />
            <rsgis:exp type="lteq" field1="string" field2="string" />
            <rsgis:exp type="eq" field1="string" field2="string" />
            <rsgis:exp type="neq" field1="string" field2="string" />
            <rsgis:exp type="gtconst" field="string" value="float" />
            <rsgis:exp type="ltconst" field="string" value="float" />
            <rsgis:exp type="gteqconst" field="string" value="float" />
            <rsgis:exp type="lteqconst" field="string" value="float" />
            <rsgis:exp type="eqconst" field="string" value="float" />
            <rsgis:exp type="neqconst" field="string" value="float" />
            <rsgis:exp type="constgt" field="string" value="float" />
            <rsgis:exp type="constlt" field="string" value="float" />
            <rsgis:exp type="constgteq" field="string" value="float" />
            <rsgis:exp type="constlteq" field="string" value="float" />
        </rsgis:if>
        <rsgis:else [field="string" value="number"] />
    </rsgis:command>
