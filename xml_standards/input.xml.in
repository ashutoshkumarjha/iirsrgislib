    <rsgis:command algor="rastergis_old" option="createattributetable" clumps="clumps.env" output="output.att" inmemory="yes | no" />
    <!-- A command to populate an attribute table with statistics from an image -->
    <rsgis:command algor="rastergis_old" option="popattributestatsinmem" input="image.env" clumps="clumps.env" table="input.att" tableout="output.att" >
        <rsgis:band band="int" [min="field"] [max="field"] [mean="field"] [sum="field"] [stddev="field"] [median="field"] />
